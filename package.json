{
  "name": "skylight",
  "displayName": "SkyLight",
  "description": "Automatically adjust theme based on light sensor.(Currently windows only)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "skylight.activate",
        "title": "Activate skylight"
      },
      {
        "command": "skylight.deactivate",
        "title": "Deactivate skylight"
      },
      {
        "command": "skylight.toggle",
        "title": "Toggle skylight"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.9",
    "@types/cron": "^2.0.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "~16.18.11",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@nodert-win10-19h1/windows.devices.sensors": "^0.1.6",
    "cron": "^2.2.0"
  }
}
